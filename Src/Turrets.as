package Src{	import flash.display.MovieClip;		public class Turrets extends MovieClip 	{		private var art:MovieClip = new TurretArt();				public function Turrets() 		{			addChild(art);			x = 400;			y = 500;		}				public function AtMouse(Xmouse:int, Ymouse:int)		{			var angleRadian=Math.atan2(Ymouse-y,Xmouse-x);			var angleDegree = angleRadian * 180 / Math.PI;			rotation = angleDegree;		}	}	}